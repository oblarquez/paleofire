    caTools,
    Imap,
    raster,
    rgdal,
    ggplot2,
    lattice,
    RColorBrewer,
    plyr,
    sp,
    pscl,
    agricolae
Depends:
    R(>= 2.10.0),
    methods,
    GCD,
    rworldmap
LazyLoad: yes
LazyData: no
Encoding: UTF-8
